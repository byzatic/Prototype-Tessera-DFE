{
   "version":1,
   "formatters":{
      "verbose":{
         "format":"%(asctime)s [%(levelname)s] %(name)s %(message)s",
         "datefmt":"%Y-%m-%d %H:%M:%S"
      },
      "simple":{
         "format":"%(levelname)s %(message)s"
      }
   },
   "handlers":{
      "Application-handler":{
         "class":"logging.handlers.RotatingFileHandler",
         "formatter":"verbose",
         "encoding":"utf8",
         "level":"DEBUG",
         "filename":"logs/application.log",
         "maxBytes":52428800,
         "backupCount":7
      },
      "LibByzaticCommon-handler":{
         "class":"logging.handlers.RotatingFileHandler",
         "formatter":"verbose",
         "encoding":"utf8",
         "level":"DEBUG",
         "filename":"logs/libbyzaticcommon.log",
         "maxBytes":52428800,
         "backupCount":7
      },
      "console":{
         "class":"logging.StreamHandler",
         "level":"DEBUG",
         "formatter":"verbose",
         "stream":"ext://sys.stdout"
      },
      "ApiPrometheus-StreamHandler": {
        "class": "logging.StreamHandler",
        "stream": "",
        "formatter": "verbose"
      }
   },
   "loggers":{
      "Application-logger":{
         "handlers":[
            "Application-handler",
            "console"
         ],
         "level":"ERROR"
      },




